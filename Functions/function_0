#include <iostream>
using namespace std;
//Find the maximum of two numbers by the function.
int maximum(int x, int y)
{
	int z;
	if (x>y)
		z = x;
	else
		z = y;
	
	return z;
}

int main()
{ 
	int a, b, max;
	cout<<"enter your digits: ";
	cin>>a>>b;
	max = maximum(a,b);
	cout<<"max is: "<<max;
	

	getchar();
	return 0;
}
